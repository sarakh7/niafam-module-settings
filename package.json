{
  "name": "module-settings",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:root": "cross-env VITE_BASE_PATH=/ vite build",
    "build:starling": "cross-env VITE_BASE_PATH=/uploads/starling/ vite build",
    "preview": "vite preview",
    "sass": "sass src/assets/scss/contents.scss public/assets/css/espritstyle.css --watch",
    "sass:about": "sass src/assets/scss/about.scss public/assets/css/about.css --watch",
    "sass:auth": "sass src/assets/scss/auth.scss public/assets/css/auth.css --watch",
    "sass:general": "sass src/assets/scss/general.scss public/assets/css/general.css --watch",
    "sass:contents-list": "sass src/assets/scss/contents-list.scss public/assets/css/contents-list.css --watch",
    "sass:search-results": "sass src/assets/scss/search-results.scss public/assets/css/search-results.css --watch",
    "sass:gallery": "sass src/assets/scss/gallery.scss public/assets/css/gallery.css --watch"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "sass": "^1.89.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "dompurify": "^3.3.0",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.3",
    "justified-layout": "^4.1.0",
    "lightgallery": "^2.8.3",
    "micromodal": "^0.6.1",
    "nouislider": "^15.8.1",
    "perfect-scrollbar": "^1.5.6",
    "plyr": "^3.7.8"
  }
}
